
<% @projects.each do |project|%>

<div class="project_container">
  <div class="container">
    <div class="title-project">
      <%= project.title %>
    </div>
      <%=link_to "See the project", project_path(@project) %>
    <div class="project-description">
      <%= project.description %>
    </div>
    <div class="row">

      <% project.jobs.each do |job| %>
      <div class="col-md-4">
        <div class="rounded-job">
         <span><%= job.name %></span>
        </div>
      </div>
      <% end %>
    </div>
  </div>
</div>
<% end %>
